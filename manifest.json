{
    "name": "Request Control",
    "description": "redirect and modify web requests",
    "options_page": "index.html",
    "manifest_version": 3,
    "version": "1.0.0",
    "action": {},
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "declarativeNetRequest",
        "browsingData"
    ],
    "host_permissions": [
        "*://*/*"
    ],
    "icons": {
        "128": "icon.png"
    }
}
